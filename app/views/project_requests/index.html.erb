<h1>Listing project_requests</h1>

<table>
  <thead>
    <tr>
      <th>Project</th>
      <th>Contact</th>
      <th>Department</th>
      <th>Location</th>
      <th>Project start</th>
      <th>Project end</th>
      <th>Skill</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @project_requests.each do |project_request| %>
      <tr>
        <td><%= project_request.project_id %></td>
        <td><%= project_request.contact_id %></td>
        <td><%= project_request.department_id %></td>
        <td><%= project_request.location_id %></td>
        <td><%= project_request.project_start %></td>
        <td><%= project_request.project_end %></td>
        <td><%= project_request.skill_id %></td>
        <td><%= link_to 'Show', project_request %></td>
        <td><%= link_to 'Edit', edit_project_request_path(project_request) %></td>
        <td><%= link_to 'Destroy', project_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project request', new_project_request_path %>
