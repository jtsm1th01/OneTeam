<%= form_for(@project) do |f| %>

  <div class="field">
    <%= f.label :project_name, 'Project Name' %><br>
    <%= f.text_field :project_name %>
  </div>
  <div class="field">
    <%= f.label :employee_id, 'Project Lead' %><br>
    <%= f.collection_select(:employee_id, @employees, :id, :employee_name, {prompt: 'Select...'}) %>
  </div>
  <div class="field">
    <%= f.label :project_start, 'Project Start Date' %><br>
    <%= f.date_select :project_start %>
  </div>
  <div class="field">
    <%= f.label :project_end, 'Project End Date' %><br>
    <%= f.date_select :project_end %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>

<% end %>
