<%= form_for(@location) do |f| %>

  <%= render 'shared/error_messages', obj: @location %>
  
  <div class="field">
    <%= f.label :location_name, 'Location Name' %><br>
    <%= f.text_field :location_name %>
  </div>
  <div class="actions">
    <%= f.submit class: 'btn btn-submit btn-primary' %>
  </div>
<% end %>


